package com.pctc.model;

import java.util.Date;

public class Student {
    private Long sId;

    private Long sInsid;

    private String sUsername;

    private String sPassword;

    private String sCreateBy;

    private Date sCreateTime;

    private String sModifyBy;

    private Date sModifyTime;

    private Boolean sDelete;

    private Integer sVersion;

    private Integer sPermissionNo;

    public Long getsId() {
        return sId;
    }

    public void setsId(Long sId) {
        this.sId = sId;
    }

    public Long getsInsid() {
        return sInsid;
    }

    public void setsInsid(Long sInsid) {
        this.sInsid = sInsid;
    }

    public String getsUsername() {
        return sUsername;
    }

    public void setsUsername(String sUsername) {
        this.sUsername = sUsername == null ? null : sUsername.trim();
    }

    public String getsPassword() {
        return sPassword;
    }

    public void setsPassword(String sPassword) {
        this.sPassword = sPassword == null ? null : sPassword.trim();
    }

    public String getsCreateBy() {
        return sCreateBy;
    }

    public void setsCreateBy(String sCreateBy) {
        this.sCreateBy = sCreateBy == null ? null : sCreateBy.trim();
    }

    public Date getsCreateTime() {
        return sCreateTime;
    }

    public void setsCreateTime(Date sCreateTime) {
        this.sCreateTime = sCreateTime;
    }

    public String getsModifyBy() {
        return sModifyBy;
    }

    public void setsModifyBy(String sModifyBy) {
        this.sModifyBy = sModifyBy == null ? null : sModifyBy.trim();
    }

    public Date getsModifyTime() {
        return sModifyTime;
    }

    public void setsModifyTime(Date sModifyTime) {
        this.sModifyTime = sModifyTime;
    }

    public Boolean getsDelete() {
        return sDelete;
    }

    public void setsDelete(Boolean sDelete) {
        this.sDelete = sDelete;
    }

    public Integer getsVersion() {
        return sVersion;
    }

    public void setsVersion(Integer sVersion) {
        this.sVersion = sVersion;
    }

    public Integer getsPermissionNo() {
        return sPermissionNo;
    }

    public void setsPermissionNo(Integer sPermissionNo) {
        this.sPermissionNo = sPermissionNo;
    }

	public Student() {
		
	}

	public Student(Long sInsid, String sUsername, String sPassword, Date sCreateTime, Boolean sDelete) {
		this.sInsid = sInsid;
		this.sUsername = sUsername;
		this.sPassword = sPassword;
		this.sCreateTime = sCreateTime;
		this.sDelete = sDelete;
	}

	@Override
	public String toString() {
		return "Student [sId=" + sId + ", sInsid=" + sInsid + ", sUsername=" + sUsername + ", sPassword=" + sPassword
				+ ", sCreateBy=" + sCreateBy + ", sCreateTime=" + sCreateTime + ", sModifyBy=" + sModifyBy
				+ ", sModifyTime=" + sModifyTime + ", sDelete=" + sDelete + ", sVersion=" + sVersion
				+ ", sPermissionNo=" + sPermissionNo + "]";
	}
    
	
    
}